<style>
    /* SVG 要素を、表示域内にフィットさせるための、SVG 要素の viewBox 属性との組み合わせを前提としたスタイル指定*/
    svg {
        width: 100%;
        height: 100%;
    }

    /* SVG 図中、".NET CLR (mono.wasm)" の図上でカーソル形状をポインタにし、クリック可能な様子を示す */
    #img4 {
        cursor: pointer;
    }
</style>

<div style="
    border:solid 1px darkorange; 
    position: relative;
    width: 100%;">

    <!-- 以下の SVG は PowerPoint でスライド内容を [図形の保存] からSVG形式で保存することで作成 -->
    <!-- ただし width と height 属性を取り除き、代わりに viewBox 属性を書き足してある (表示域にフィットさせるため) -->
    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 1301 715">
        <defs>
            <clipPath id="clip0">
                <rect x="5" y="13" width="1301" height="715" />
            </clipPath>
            <image width="580" height="225"
                xlink:href="data:image/png;base64,"
                preserveAspectRatio="none" id="img1"></image>
            <image width="292" height="182"
                xlink:href="data:image/png;base64,"
                preserveAspectRatio="none" id="img2"></image>
            <image width="292" height="181"
                xlink:href="data:image/png;base64,"
                preserveAspectRatio="none" id="img3"></image>
            <image width="292" height="182"
                xlink:href="data:image/png;base64,"
                preserveAspectRatio="none" id="img4"></image>
            <image width="292" height="182"
                xlink:href="data:image/png;base64,"
                preserveAspectRatio="none" id="img5"></image>
        </defs>
        <g clip-path="url(#clip0)" transform="translate(-5 -13)"><text
                font-family="Yu Gothic UI,Yu Gothic UI_MSFontService,sans-serif" font-weight="400" font-size="64"
                transform="translate(44.1936 96)">仕組み</text>
            <use width="100%" height="100%" xlink:href="#img1" fill="none" transform="translate(349 439)"></use>
            <use width="100%" height="100%" xlink:href="#img2" fill="none" transform="translate(348 306)"></use>
            <use width="100%" height="100%" xlink:href="#img3" fill="none" transform="translate(634 350)"></use>
            <use width="100%" height="100%" xlink:href="#img4" fill="none" transform="translate(634 218)"></use>
            <use width="100%" height="100%" xlink:href="#img5" fill="none" transform="translate(634 88)"></use><text
                fill="#3F3F3F" font-family="MS Gothic,MS Gothic_MSFontService,sans-serif" font-weight="400"
                font-size="24" transform="translate(447.053 706)">※<tspan
                    font-family="Yu Gothic UI,Yu Gothic UI_MSFontService,sans-serif" font-size="24" x="24" y="0">
                    このような動作原理なので、既存の</tspan>
                <tspan font-family="Segoe UI,Segoe UI_MSFontService,sans-serif" font-size="24" x="359.5" y="0">NuGet
                </tspan>
                <tspan font-family="Yu Gothic UI,Yu Gothic UI_MSFontService,sans-serif" font-size="24" x="434.833"
                    y="0">パッケージライブラリもそのまま使えたりする。</tspan>
            </text>
        </g>
    </svg>

</div>

@*
    [注意] index.html にて、以下の内容の JavaScript コードを読み込んでおく必要があります。
    <script>
    helper = {
    obj: null,
    attach: (obj) => helper.obj = obj
    };
    document.addEventListener('click', e => {
    if (e.srcElement === document.querySelector("use[*|href='#img4']") && helper.obj !== null) {
    helper.obj.invokeMethodAsync('OnClickMono');
    }
    });
    </script>
*@

@inject IJSRuntime JS
@implements IAsyncDisposable
@code {
    private DotNetObjectReference<App> _objRef;

    protected override async Task OnInitializedAsync()
    {
        _objRef = DotNetObjectReference.Create(this);
        await JS.InvokeVoidAsync("helper.attach", _objRef);
    }

    [JSInvokable]
    public async Task OnClickMono()
    {
        await JS.InvokeVoidAsync("alert", "'.NET CLR (mono.wasm)' was clicked.");
    }

    public async ValueTask DisposeAsync()
    {
        await JS.InvokeVoidAsync("helper.attach", null);
        _objRef?.Dispose();
    }
}
